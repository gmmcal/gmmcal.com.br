<!DOCTYPE html>
<!--[if lt IE 7 ]> <html class="ie ie6 no-js" lang="<%=I18n.locale%>"> <![endif]-->
<!--[if IE 7 ]>    <html class="ie ie7 no-js" lang="<%=I18n.locale%>"> <![endif]-->
<!--[if IE 8 ]>    <html class="ie ie8 no-js" lang="<%=I18n.locale%>"> <![endif]-->
<!--[if IE 9 ]>    <html class="ie ie9 no-js" lang="<%=I18n.locale%>"> <![endif]-->
<!--[if gt IE 9]><!--><html class="no-js" lang="<%=I18n.locale%>"><!--<![endif]--><head>
  <meta charset="UTF-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <title>Gustavo Cunha - Software Engineer</title>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta name="description" content="<%=ActionView::Base.full_sanitizer.sanitize(t(:text, scope: [:about]))%>" />
  <!-- Open Graph Tags -->
  <meta property="og:title" content="Gustavo Cunha - Software Engineer" />
  <meta property="og:site_name" content="Gustavo Cunha"/>
  <meta property="og:image" content="<%=asset_url('website/avatar.jpg')%>"/>
  <meta property="og:url" content="<%=home_url(I18n.locale)%>" />
  <meta property="og:description" content="<%=ActionView::Base.full_sanitizer.sanitize(t(:text, scope: [:about]))%>" />
  <meta property="og:type" content="website" />
  <meta property="og:locale" content="<%=I18n.locale%>" />
  <!--google font style-->
  <!--font-family: 'Open Sans', sans-serif;-->
  <link href='https://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,800italic,400,600,700,800,300' rel='stylesheet' type='text/css'>
  <%= Gon::Base.render_data %>
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_pack_tag 'application', type: 'application/javascript' %>

  <noscript>
  <%= stylesheet_link_tag 'fallback', media: 'all', 'data-turbolinks-track' => true %>
  </noscript>
  <!--[if lt IE 9]>
    <%= stylesheet_link_tag 'fallback', media: 'all', 'data-turbolinks-track' => true %>
  <![endif]-->
</head>
<body id="home">
  <div class="wrapper" id="wrapper" itemscope itemtype="https://schema.org/Person">
    <vue-header inline-template>
      <header>
        <ul class="i18n">
          <li v-for="item in flag_links" :key="item.id">
            <menu-item :item="item" />
          </li>
        </ul>
        <!--banner start-->
        <div class="banner">
          <div class="row">
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 no-padd" style="height:100%">
              <!--background slide show start-->
              <div class="banner-part"> <%= image_tag "website/banner.jpg", class: "banner-img" %> </div>
              <!--background slide show end-->
            </div>
          </div>
          <!--banner end-->
          <div class="banner-text container">
            <h1><%=t(:name, scope: [:template])%></h1>
            <h2><%=t(:title, scope: [:template])%></h2>
          </div>
        </div>
        <!--menu start-->
        <div class="menu">
          <div class="navbar-wrapper is-stuck">
            <div class="container">
              <div class="navwrapper">
                <div class="navbar navbar-inverse navbar-static-top">
                  <div class="container">
                    <div class="navbar-header">
                      <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                      </button>
                      <a class="navbar-brand" href="#">Menu</a>
                    </div>
                    <div class="navbar-collapse collapse">
                      <ul class="nav navbar-nav">
                        <li class="menuItem" v-for="item in menu_links" :key="item.id">
                          <menu-item :item="item" />
                        </li>
                      </ul>
                    </div>
                  </div>
                </div>
              </div>
              <!-- End Navbar -->
            </div>
          </div>
        </div>
        <!--menu end-->
      </header>
    </vue-header>
    <vue-body inline-template>
      <div>
        <%= yield %>
      </div>
    </vue-body>
    <vue-footer inline-template>
      <!--footer start-->
      <section class="footer" id="footer">
        <div class="container">
          <ul>
            <li><a href="https://github.com/gmmcal" target="_blank"><i class="fa fa-github fa-2x"></i></a></li>
            <li><a href="https://linkedin.com/in/gmmcal" target="_blank"><i class="fa fa-linkedin fa-2x"></i></a></li>
            <li><a href="https://twitter.com/gmmcal" target="_blank"><i class="fa fa-twitter fa-2x"></i></a></li>
          </ul>
        </div>
      </section>
      <!--footer end-->
    </vue-footer>
  </div>
  <script>
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

  ga('create', 'UA-12085288-1', 'auto');
  ga('send', 'pageview');
  </script>
</body>
</html>
